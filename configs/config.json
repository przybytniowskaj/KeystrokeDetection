{
    "model": "coatnet",
    "model_params": {
        "nums_blocks": [2, 2, 3, 5, 2],
        "channels": [64, 96, 192, 384, 768]
    },
    "optimizer": "adam",
    "optimizer_params": {
        "lr": 0.001,
        "weight_decay": 0.0001
    },
    "scheduler": "StepLR",
    "scheduler_params": {
        "step_size": 10,
        "gamma": 0.1
    },
    "num_epochs": 5,
    "batch_size": 2048,
    "num_workers": 1,
    "max_no_improvement": 20,
    "transform_aug": true,
    "seed": 42
}